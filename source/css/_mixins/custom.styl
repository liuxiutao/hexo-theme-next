mobile-smallest() {
  @media (max-width: 400px) {
    {block}
  }
}

mobile-small() {
  @media (max-width: 520px) {
    {block}
  }
}

mobile() {
  @media (max-width: 640px) {
    {block}
  }
}

tablet() {
  @media (min-width: 641px) and (max-width: 800px) {
    {block}
  }
}

desktop() {
  @media (min-width: 801px) {
    {block}
  }
}

desktop-large() {
  @media (min-width: 1600px) {
    {block}
  }
}




